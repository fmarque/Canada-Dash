<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.group47.canadadash.InstructorDashboardController">
  <children>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
         <center>
            <TableView fx:id="studentTableView" prefHeight="400.0" prefWidth="448.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn id="studentIdColumn" fx:id="studentIdColumn" minWidth="0.0" prefWidth="173.0" text="Student Id" />
                  <TableColumn fx:id="highScoreColumn" prefWidth="203.0" text="High Score" />
                  <TableColumn fx:id="highestLevelColumn" prefWidth="176.0" text="Highest Level" />
              </columns>
            </TableView>
         </center>
         <left>
            <Button mnemonicParsing="false" onAction="#onBackButtonClicked" text="Back" BorderPane.alignment="CENTER" />
         </left>

      </BorderPane>
  </children>
</VBox>
